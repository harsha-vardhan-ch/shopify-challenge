    "scripts": {
        "replit": "yarn build && yarn start",
        "start": "node dist/shared/infra/http/server.js",
        "build": "yarn install && babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
        "dev": "tsnd -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn --exit-child src/shared/infra/http/server.ts",
        "lint": "eslint . --ext .js,.ts",
        "lint-fix": "eslint . --ext .js,.ts --fix",
        "test": "jest"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "ejs": "^3.1.8",
        "express": "^4.18.1",
        "express-async-errors": "^3.1.1",
        "helmet-csp": "^3.4.0",
        "hpp": "^0.2.3",
        "joi": "^17.6.0",
        "reflect-metadata": "^0.1.13",
        "supertest": "^6.2.3",
        "swagger-ui-express": "^4.4.0",
        "tsyringe": "^4.6.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.17.12",
        "@babel/node": "^7.17.10",
        "@babel/plugin-proposal-class-properties": "^7.17.12",
        "@babel/plugin-proposal-decorators": "^7.17.12",
        "@babel/preset-env": "^7.17.12",
        "@babel/preset-typescript": "^7.17.12",
